{
    "name": "Solarized Osaka",
    "type": "dark",
    "colors": {
        "editor.background": "#011219",
        "editor.foreground": "#a6b0b0",
        "editor.selectionBackground": "#041f2f",
        "editor.lineHighlightBackground": "#041f2f",
        "editor.selectionHighlightBackground": "#041f2f",
        "editorCursor.foreground": "#a6b0b0",
        "editorWhitespace.foreground": "#041f2f",
        "editor.findMatchBackground": "#041f2f",
        "editor.findMatchHighlightBackground": "#041f2f",
        "sideBar.background": "#011219",
        "sideBar.foreground": "#a6b0b0",
        "sideBar.border": "#011219",
        "editorWidget.background": "#011219",
        "editorWidget.border": "#011219",
        "dropdown.background": "#041f2f",
        "dropdown.border": "#011219",
        "panel.background": "#011219",
        "panel.border": "#011219",
        "statusBar.background": "#041f2f",
        "statusBar.foreground": "#a6b0b0",
        "statusBar.border": "#041f2f",
        "statusBar.noFolderBackground": "#041f2f",
        "statusBar.debuggingBackground": "#041f2f",
        "statusBarItem.remoteBackground": "#041f2f",
        "statusBarItem.remoteForeground": "#a6b0b0",
        "scrollbar.shadow": "#041f2f",
        "titleBar.activeBackground": "#011219",
        "titleBar.activeForeground": "#a6b0b0",
        "titleBar.border": "#011219",
        "activityBar.background": "#011219",
        "activityBar.foreground": "#a6b0b0",
        "activityBar.border": "#011219",
        "list.activeSelectionBackground": "#041f2f",
        "list.hoverBackground": "#041f2f",
  "editorGroupHeader.tabsBackground": "#011219",
  "tab.activeBackground": "#011219",
  "tab.inactiveBackground": "#011219",
        "tab.unfocusedActiveBackground": "#011219",
        "editorLineNumber.foreground": "#463a0b",
        "editorLineNumber.activeForeground": "#aa4316"
    },
    "semanticTokenColors": {
        "error": "#d94845",
        "warning": "#b38000",
        "info": "#2580c3",
        "hint": "#29927c",
        "todo": "#6871d4",
        "modified": "#2580c3",
        "deprecated": "#586e75"
    },
    "tokenColors": [
        {
            "scope": [
                "punctuation",
                "meta.brace",
                "punctuation.definition.block",
                "punctuation.definition.parameters",
                "meta.brackets",
                "punctuation.section"
            ],
            "settings": {
                "foreground": "#B7211F"
            }
        },
        {
            "scope": [
                "constant",
                "entity.name.constant",
                "variable.other.constant",
                "variable.language",
                "entity.name.constructor",
                "constant.numeric",
                "string"
            ],
            "settings": {
                "foreground": "#2AA198"
            }
        },
        {
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#586E75",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "keyword",
                "storage.type",
                "storage.modifier",
                "support.function",
                "markup.underline.link",
                "entity.name.tag",
                "meta.embedded"
            ],
            "settings": {
                "foreground": "#268BD2"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#29EEDF"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "scope": [
                "entity.name.section",
                "entity.name.class",
                "entity.name.namespace",
                "entity.name.module"
            ],
            "settings": {
                "foreground": "#268BD2",
                "fontWeight": "600"
            }
        }
    ]
}





